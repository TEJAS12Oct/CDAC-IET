public class number31 {

		public static void main(String[] args) {
			int i, j;
			char ch;
			ch = 'A';
			for (i = 0; i < 5; i++) {
				for (j = 0; j < 5; j++) {
					System.out.print(ch + " ");
					ch++;
				}
