package Array;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		int[] arr = new int[5];

		Scanner sc = new Scanner(System.in);
		System.out.println("Enter 5 no = ");

		for (int i = 0; i < arr.length; i++) {
			arr[i] = sc.nextInt();
		}
		System.out.println("arr[3] = " + arr[3]);
	}

}
