package Array2D;

public class Q3 {

	public static void main(String[] args) {
		int twoarr[][] = new int[3][3];
		int i, j, n = twoarr.length, sum = 0;
		for (i = 0; i < twoarr.length; i++) {
			for (j = 0; j < twoarr[i].length; j++) {
				if ((i == 0) || (j == 0) || (i == n - 1) || (j == n - 1))
					sum = sum + twoarr[i][j];
			}
		}
		System.out.println(sum);

	}

}
