// Array reverse
package Array1D;

public class Q5 {

	public static void main(String[] args) {
		int i, j, temp;
		int arr[] = { 1, 2, 3, 4, 5, 6 };

		for (i = 0 , j =  arr.length - 1; i < j; i++, j--) {
			temp = arr[i];
			arr[i] = arr[j];
			arr[j] = temp;
			System.out.println(arr[i]);

		}
		
	}

}
