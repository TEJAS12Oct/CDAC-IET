package Q2;

public class Number1 {
	private int n;

	public Number1(int n) {
		this.n = n;

	}

	public Number1() {

	}

	public int getNumber1() {
		return this.n = n;
	}

	public void setNumber1(int n) {
		this.n = n;
	}

	public String toString() {
		String str = "n = " + n;
		return str;
	}

}
