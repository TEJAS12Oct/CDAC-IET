package Q1;

public class Number {

	private static int n;

	public Number(int n) {
		this.n = n;

	}

	public Number() {

	}

	public static int getNumber() {
		return n;
	}

	public static void setNumber(int n) {
		Number.n = n;
	}

	public String toString() {
		String str = "n = " + n;
		return str;
	}

