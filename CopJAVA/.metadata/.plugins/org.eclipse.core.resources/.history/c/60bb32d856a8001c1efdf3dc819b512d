package Q1;

public class Number {

	private int n;

	public Number(int n) {
		this.n = n;

	}

	public Number() {

	}
	
	public static int getNumber() {
		return n;
	}
	
	public static void setNumber(int n) {
		number.n = n;
	}
	
	public String toString() {
		String str = "n1 = " + n;
		return str;
	}

}
