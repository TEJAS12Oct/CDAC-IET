public class main {

	public static void main(String[] args) {
		Number n1 = new Number();
		System.out.println("n = "+args[1]);
		
		
		

		n1 = MathOperation.addition(args[0], args[1]);
		System.out.println("Addition of " + n1 + " and " + n2 + " is : " + n3);

		n2 = MathOperation.multiply(args[0], args[1]);
		System.out.println("Multiplication of " + n3 + " and " + n2 + " is : " + n4);
		
		n3 = MathOperation.subtraction(args[0], args[1]);
		System.out.println("Addition of " + n1 + " and " + n2 + " is : " + n3);

		n4 = MathOperation.division(args[0], args[1]);
		System.out.println("Multiplication of " + n3 + " and " + n2 + " is : " + n4);

		if (n4.greaterThan(n1)) {
			n1.setNumber(n4.getNumber());
		}

		System.out.println("Finally n1 = " + n1);
	}

}
