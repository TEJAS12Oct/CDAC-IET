package Q2;

import Q1.MathOperation;
import Q1.Number;

public class main {

	public static void main(String[] args) {
		Number n = new Number();
		int x = Integer.parseInt(args[0]);
		int y = Integer.parseInt(args[1]);

		Number n5 = MathOperation.addition(args[0], args[1]);
		System.out.println("Addition of " + args[0] + " and " + args[1] + " is : " + n5);

		Number n6 = MathOperation.multiply(args[0], args[1]);
		System.out.println("Multiplication of " + args[0] + " and " + args[1] + " is : " + n6);

		Number n3 = MathOperation.subtraction(args[0], args[1]);
		System.out.println("Addition of " + args[0] + " and " + args[1] + " is : " + n3);

		Number n4 = MathOperation.division(args[0], args[1]);
		System.out.println("Multiplication of " + args[0] + " and " + args[1] + " is : " + n4);

	}

}
