package CircleLab;

public class Point {
	int CenterX, CenterY;

	public Point() {

	}

	public Point(int x, int y) {
		CenterX = x;
		CenterY = y;
	}

	public void setCenter(int x, int y) {
		CenterX = x;
		CenterY = y;
	}

	public int getCenter() {
		return CenterX;
	}

	public int getCenter() {
		return Centery;
	}
}
