package Circle;

public class Circle {
	int radius;

	public Circle() {
		radius = 3;

	}
	
	public Circle (int x) {
		radius =  x;
		
	}
	public void setRadius(int r) {
		radius = r;
	}
	public int getRadius() {
		return radius;
	}
	
	public String toString() {
		String str = "Radius = " + radius;
				return str;
	}

}
