package Q2;

import Q1.MathOperation;
import Q1.Number;

public class main {

	public static void main(String[] args) {
		Number n = new Number();
		System.out.println("n = "+args[1]);
		int n1 = args[0];
		int n2 = args[1];
		
		
		

		Number n1 = MathOperation.addition(n1, n2);
		System.out.println("Addition of " + n1 + " and " + n2 + " is : " + n1);

		Number n2 = MathOperation.multiply(n1, n2);
		System.out.println("Multiplication of " + n1] + " and " + n2 + " is : " + n2);
		
		Number n3 = MathOperation.subtraction(n1, n2);
		System.out.println("Addition of " + n1 + " and " + n2 + " is : " + n3);

		Number n4 = MathOperation.division(n1, n2);
		System.out.println("Multiplication of " + n1 + " and " + n2 + " is : " + n4);

		if (n4.greaterThan(n1)) {
			n1.setNumber(n4.getNumber());
		}

		System.out.println("Finally n1 = " + n1);
	}

}
