package Q3;

public class Rectangle {
	int l;
	int w;

	public Rectangle(int x, int y) {
		l = x;
		w = y;
	}

	void setl(int setl) {
		l = setl;
	}

	public int getl() {
		return l;
	}

	void setW(int setW) {
		w = setW;
	}

	public int getW() {
		return w;
	}

	void area() {
		int area = l * w;
		System.out.println("Area Of Rectangle = " + area);
	}

	void display() {

		System.out.println("Length = " + l + "  Width = " + w);

	}

	public String toString() {
		String str = ("Length = " + l + "  Width = " + w + "   Area = " + (l * w));
		return str;
	}
}
