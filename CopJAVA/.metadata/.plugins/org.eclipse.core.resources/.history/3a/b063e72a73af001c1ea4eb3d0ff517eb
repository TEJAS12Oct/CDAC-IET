package PATTERN;

public class Q16 {

	public static void main(String[] args) {
		int x;
		int y;
		int n=5;
	
		for(int i=1;i<=n;i++)
		{
			x=i;
			y=n-i+1;
			for(int j=1;j<=n;j++)
			{
				if(j%2==1)
				{
					System.out.printf("%3d",x);
				}
				else
				{
					System.out.printf("%3d",y);
				}
				x+=n;
				y+=n;
				
			}
			System.out.println();

	}

}
