package Q1;

public class MathOperation {
	static int n3;
	static int n4;

	public static int addititon(Number n1, Number n2) {
		n3 = Number.getNumber() + Number.getNumber();
		return n3;
	}

	public static Number multiply(Number n3, Number n2) {
		n4 = Number.getNumber() * Number.getNumber();
		return Number.multiply();
	}

}
