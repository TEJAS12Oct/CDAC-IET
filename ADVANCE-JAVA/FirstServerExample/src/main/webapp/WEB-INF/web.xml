<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>FirstServerExample</display-name>

	<servlet>
		<servlet-name>ex1</servlet-name>
		<servlet-class>Study.MyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ex1</servlet-name>
		<url-pattern>/First</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ex2</servlet-name>
		<servlet-class>Study.SecondServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ex2</servlet-name>
		<url-pattern>/Second</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ex3</servlet-name>
		<servlet-class>Study.InputServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ex3</servlet-name>
		<url-pattern>/Input</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ex4</servlet-name>
		<servlet-class>Study.Third</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ex4 </servlet-name>
		<url-pattern>/Third</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>ex5</servlet-name>
		<servlet-class>Study.LifeCycleEx</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ex5 </servlet-name>
		<url-pattern>/Life</url-pattern>
	</servlet-mapping>

</web-app>