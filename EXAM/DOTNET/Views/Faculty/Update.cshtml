@using Question1.Models;

@{
    ViewBag.Title = "Update";
}

@{ Presenter work = this.Model;}
<h2>Update Workshop Detail</h2>
<form method="post" action="/faculty/update">


    <table>
       
        <tr>
            <td>Topic:</td>
            <td><input type="text" name="topic" value="@work.Topic" readonly /></td>
        </tr>
        <tr>
            <td>Description:</td>
            <td><input type="text" name="Description" value="@work.Description" /></td>
        </tr>
        <tr>
            <td>Faculty:</td>
            <td><input type="text" name="Faculty" value="@work.Faculty" /></td>
        </tr>
        <tr>
            <td>Location:</td>
            <td><input type="text" name="Location" value="@work.Location" /></td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" /></td>
        </tr>
    </table>
</form>

