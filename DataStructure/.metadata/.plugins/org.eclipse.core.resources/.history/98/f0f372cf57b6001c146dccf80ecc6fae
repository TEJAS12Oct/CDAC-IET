package StudentStackLLMD;

public class Student {

	private String name;
	private int age;
	private int gender;

	Student(String name, int age, int gender) {
		this.name = name;
		this.age = age;
		this.gender = gender;
	}

	public String toString() {
		String str = "";
		str = name + " ," + age + " ," + gender;
		return str;
	}

}
