package Student3;

public class Main {

	public static void main(String[] args) {
		
		Student[] students = new Student[5];
		
		Student s1 = new Student("Tejas", 22, 'M', 340);
		Student s2 = new Student("Aniket", 25, 'M', 370);
		Student s3 = new Student("Roshan", 27, 'M', 400);
		Student s4 = new Student("Nisha", 20, 'F', 300);
		Student s5 = new Student("Sayali", 24, 'F', 335);

		Sort S = new Sort();
		S[0] = s1;
		Sort[1] = s2;
		Sort[2] = s3;
		Sort[3] = s4;
		Sort[4] = s5;

		System.out.println("******************Display Information Student **************");
		Sort.print(students);

		Sort.NameAscending(students);
		Sort.print();

		Sort.NameDecending(students);
		Sort.print();

		Sort.AgeAscending(students);
		Sort.print();

		Sort.AgeDecending(students);
		Sort.print();

		Sort.MarksAscending(students);
		Sort.print();

		Sort.MarksDecending(students);
		Sort.print();

	}
}
