package StackMD;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		int choice, N;

		Scanner sc = new Scanner(System.in);
		choice = sc.nextInt();

		int size = sc.nextInt();

		Stack s = new Stack(size);
		Stack s2 = new Stack();

		do {
			N = sc.nextInt();
			switch (choice) {

			case 1:
				s2.push(N);
				break;

			case 2:
				s2.pop(N);
				break;

			case 3:
				s2.concate(s2);
				break;

			case 4:
				equals(s2);
				break;

			default:

			}

		} while (choice == 5);
	}

	private int[] arr;
	private int top;

	
	

	public String toString() {

		String str = "";
		if (this.top == -1)
			str = "Stack is empty";
		else {
			for (int i = this.top; i >= 0; i--)
				str = str + (this.arr[i] + ", ");
		}

		return str;
	}

	


}
