package Student;

public class Student {
	String name;
	int age;
	char gender;
	int marks;

	Student(String name, int age, char gender, int marks) {

		this.name = name;
		this.age = age;
		this.gender = gender;
		this.marks = marks;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public char getGender() {
		return gender;
	}

	public void setGender(char gender) {
		this.gender = gender;
	}

	public int getMarks() {
		return marks;
	}

	public void setMarks(int marks) {
		this.marks = marks;
	}

	public String toString() {
		String str = "Student [name=" + name + ", age=" + age + ", gender=" + gender + ", marks=" + marks + "]";
		return str;
	}
	
	public static void print(Student[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i] + " ,");
		}
		System.out.println();
	}

	public static void AgeAscending(Student[] students) {
		// TODO Auto-generated method stub
		
	}

	public static void MarksAscending(Student[] students) {
		// TODO Auto-generated method stub
		
	}

	public static void MarksDecending(Student[] students) {
		// TODO Auto-generated method stub
		
	}

	public static void AgeDecending(Student[] students) {
		// TODO Auto-generated method stub
		
	}

}
