package Employee;

public class Main {
	public static void main(String[] args) {
		Employee E = new Employee();

		E = new Employee(name, id, Salary);
		E.append("Tejas", 4, 45000);
		E.insert("Tejas", 1, 45000);
		E.append("Tejas", 5, 45000);
		E.insert("Tejas", 2, 45000);
		E.append("Tejas", 6, 45000);
		E.insert("Tejas", 3, 45000);

		System.out.println(E);

		E.deleteFirst();
		E.deleteLast();

		System.out.println(E);
	}

}
