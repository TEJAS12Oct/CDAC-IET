package Student;

public class Student {
	String name;
	int age;
	char gender;
	int marks;

	Student(String name, int age, char gender, int marks) {

		this.name = name;
		this.age = age;
		this.gender = gender;
		this.marks = marks;
	}

	public static void NameAscending(Student[] students) {

