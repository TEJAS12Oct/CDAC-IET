package Employee;

public class Node {

	Employee data;
	Node next;

	public Node() {
		data = 0;
		next = null;
	}

	public Node(Employee data) {
		this.data = data;
		this.next = null;
	}

	public Node(Node next) {
		this.data = 0;
		this.next = next;
	}

	public Node(Employee data, Node next) {
		this.data = data;
		this.next = next;
	}

}
