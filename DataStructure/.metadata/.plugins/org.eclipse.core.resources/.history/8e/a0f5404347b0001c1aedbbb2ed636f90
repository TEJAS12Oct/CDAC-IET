// Function to display and find out the sum of series
// Series : 1 + 2 + 3 + 4 + 5 +....... 

package Assignment2;

public class Q1 {

	public static void main(String[] args) {

		int i;
		sum(0);

	}

	public static void sum(int i) {

		if (i < 10) {
			for (i = 1; i <= 10; i++) {
				System.out.print(" "  + i);
				
				
			}
		}
	}

}
