package Student1;

import java.util.Scanner;

import Student.Stack;

public class Main1 {

	public static void main(String[] args) {

		String name = null;
		int age = 0;
		char gender = null;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter Your Size = ");
		int size = sc.nextInt();

		Student1[] s = new Student1[size];

		for (int i = 0; i < size; i++) {

			System.out.println("ENTER NAME = ");
			name = sc.next();

			System.out.println("ENTER AGE = ");
			age = sc.nextInt();

			System.out.println("ENTER GENDER = ");
			gender = sc.next().charAt(0);
		}

		Student1 S = new Student1();

		S.setname(name);
		S.setage(age);
		S.setgender(gender);

		Stack1 StuStack = new Stack1(size);

		StuStack.push(S);
		StuStack.push(S);
		// StuStack.push(S);
		// StuStack.push(S);

		System.out.println(" after push " + StuStack);

		StuStack.pop(S);
		System.out.println(" after pop " + StuStack);

	}

}
