// Function to display and find out the sum of series
// Series : 1 + 2 + 3 + 4 + 5 +....... 

package Assignment2;

public class Q1 {

	public static void main(String[] args) {

		int i,n = 10;
		sum(i);

	}

	public static void sum(int i) {
		
		if(i<n) {
			System.out.println(i + ",");
		}

	}

}
